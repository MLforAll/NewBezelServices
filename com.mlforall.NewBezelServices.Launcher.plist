<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>Label</key>
	<string>com.mlforall.NewBezelServices.Launcher</string>
	<key>ProgramArguments</key>
	<array>
		<string>/bin/bash</string>
		<string>-c</string>
		<string>launchctl unload /System/Library/LaunchAgents/com.apple.OSDUIHelper.plist /Library/LaunchAgents/com.mlforall.NewBezelServices.plist; launchctl load /Library/LaunchAgents/com.mlforall.NewBezelServices.plist</string>
	</array>
	<key>LimitLoadToSessionType</key>
	<array>
		<string>LoginWindow</string>
		<string>Aqua</string>
	</array>
	<key>KeepAlive</key>
	<false/>
	<key>RunAtLoad</key>
	<true/>
</dict>
</plist>
